:output '((:data) (:lilypond :process t :view t))

:lilypond-exe "/opt/homebrew/bin/lilypond"
:lilypond-opts '("--pdf")
:lilypond-out-ext "pdf"

:lilypond-view-exe "/usr/bin/open"
:lilypond-view-opts '("-a" "Preview.app")
:cmn-view-exe "/usr/bin/open"
:cmn-view-opts '("-a" "Preview.app")

:verbose 1
:min-tuplet-dur 1
:max-tuplet-dur 1
:max-tuplet '(5)
:caut-acc-ottavas t
:auto-timesig-comp nil
:auto-beams nil
:auto-override-timesigs nil
